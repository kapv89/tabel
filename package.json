{
  "name": "tabel",
  "version": "0.7.0",
  "repository": "https://github.com/fractaltech/tabel",
  "description": "A simple orm with over knex.js which works with simple javascript objects and arrays",
  "main": "lib/Orm.js",
  "scripts": {
    "test": "babel-node --stage 1 test/orm/main.js",
    "test:collisions": "babel-node --stage 1 test/collisions/main.js",
    "build": "npm run clean && babel --stage 1 -D src/ --out-dir lib/",
    "watch": "npm run clean && babel --stage 1 -w -D src/ --out-dir lib/",
    "clean": "rm -rf lib/"
  },
  "author": "Kapil Verma <kapv89@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "isusableobject": "^0.1.1",
    "knex": "~0.9.0",
    "kredis": "^1.0.1",
    "lodash": "^3.10.1",
    "md5": "^2.0.0",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "babel": "^5.8.34",
    "babel-eslint": "^4.1.6",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.12.0",
    "faker": "^3.0.1",
    "mysql": "^2.10.2",
    "pg": "^4.4.3"
  }
}
